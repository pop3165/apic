
<ncs-package xmlns="http://tail-f.com/ns/ncs-packages">
  <name>cisco-ios</name>
  <package-version>3.0</package-version>
  <description>NED package for Cisco IOS</description>
  <ncs-min-version>3.0.2</ncs-min-version>
  <component>
    <name>cisco-ios</name>
    <ned>
      <cli>
        <ned-id xmlns:ios-id="urn:ios-id">ios-id:cisco-ios</ned-id>
        <java-class-name>com.tailf.packages.ned.ios.IOSNedCli</java-class-name>
      </cli>
      <device>
        <vendor>Cisco</vendor>
      </device>
      <option>
        <name>show-tag</name>
        <value>interface</value>
      </option>
    </ned>
  </component>
   <component>
    <name>upgrade-ned-id</name>
    <upgrade>
      <java-class-name>com.tailf.packages.ned.ios.UpgradeNedId</java-class-name>
    </upgrade>
  </component>
</ncs-package>
